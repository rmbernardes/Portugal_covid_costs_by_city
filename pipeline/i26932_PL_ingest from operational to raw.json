									}
								}
							],
							"outputs": [
								{
									"referenceName": "i26932_DS_ADLS_FROMCONFIG",
									"type": "DatasetReference",
									"parameters": {
										"TABLE_NAME": "@item().SourceTable"
									}
								}
							]
						}
					]
				}
			}
		],
